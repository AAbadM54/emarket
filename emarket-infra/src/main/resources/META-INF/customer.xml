<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
        xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm
                  http://xmlns.jcp.org/xml/ns/persistence/orm_2_1.xsd"
        version="2.1"
>
    <access>FIELD</access>
    <entity class="com.cristi.web.emarket.domain.customer.Customer">
        <attributes>
            <
        </attributes>
    </entity>
    <embeddable class="com.cristi.web.emarket.domain.UniqueId">
        <attributes>
            <basic name="value">
                <column name="ID"/>
            </basic>
        </attributes>
    </embeddable>

    <embeddable class="com.cristi.web.emarket.domain.Description">
        <attributes>
            <basic name="value">
                <column name="DESCRIPTION"/>
            </basic>
        </attributes>
    </embeddable>

    <embeddable class="com.cristi.web.emarket.domain.customer.NamePart">
        <attributes>
            <basic name="value">
                <column name="NAME_VALUE"/>
            </basic>
        </attributes>
    </embeddable>
    <embeddable class="com.cristi.web.emarket.domain.customer.CustomerName">
        <attributes>
            <embedded name="firstName">
                <attribute-override name="value">
                    <column name="FIRST_NAME"/>
                </attribute-override>
            </embedded>
            <embedded name="lastName">
                <attribute-override name="value">
                    <column name="LAST_NAME"/>
                </attribute-override>
            </embedded>
        </attributes>
    </embeddable>
</entity-mappings>
